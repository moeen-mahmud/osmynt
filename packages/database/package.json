{
	"name": "@osmynt-core/database",
	"version": "1.1.15",
	"main": "index.ts",
	"module": "index.ts",
	"type": "module",
	"private": true,
	"scripts": {
		"prepare-env": "NODE_ENV=development bun --env-file=../../.env run",
		"db:generate": "prisma generate",
		"db:migrate": "bun run prepare-env prisma migrate dev",
		"db:deploy": "bun run prepare-env prisma migrate deploy",
		"db:studio": "bun run prepare-env prisma studio",
		"db:push": "bun run prepare-env prisma db push"
	},
	"dependencies": {
		"@prisma/client": "^6.8.2"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"prisma": "^6.8.2"
	},
	"peerDependencies": {
		"typescript": "^5"
	}
}
