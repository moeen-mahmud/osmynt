generator client {
  provider        = "prisma-client-js"
  output          = "../generated/client"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}



model AuditLog {
  id        String   @id @default(cuid())
  action    String
  // user      User     @relation(fields: [userId], references: [id])
  userId    String
  metadata  Json
  createdAt DateTime @default(now())
}
