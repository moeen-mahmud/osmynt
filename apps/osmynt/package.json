{
	"main": "dist/extension.cjs",
	"dependencies": {
		"@types/bun": "^1.1.13",
		"@types/vscode": "^1.86.0",
		"@supabase/supabase-js": "^2.58.0"
	},
	"author": {
		"name": "Moeen Mahmud",
		"email": "moeen.mahmud007@gmail.com",
		"url": "https://github.com/moeen-mahmud"
	},
	"devDependencies": {
		"@types/bun": "1.1.13",
		"@osmynt-core/api": "workspace:*",
		"@osmynt-core/library": "workspace:*",
		"bun": "1.1.34",
		"@types/ws": "^8.5.12",
		"@vscode/vsce": "^3.2.1",
		"esbuild": "^0.20.0"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	},
	"type": "module",
	"scripts": {
		"prepare-env": "NODE_ENV=development bun --env-file=../../.env run",
		"dev": "echo 'Open this directory in VSCode and then run your extension with F5 or `Run and Debug > Run Extension`!'",
		"build": "bun run prepare-env ./scripts/build-with-esbuild.ts",
		"watch": "bun run prepare-env ./scripts/watch-with-esbuild.ts",
		"vsce-package": "bun run prepare-env vsce package --no-dependencies",
		"package": "bun run prepare-env build && bun run vsce-package",
		"clean": "rm -r ./dist"
	},
	"publisher": "Osmynt",
	"name": "osmynt",
	"version": "0.17.3",
	"displayName": "Osmynt Beta",
	"description": "Real-time snippet sharing through Osmynt",
	"categories": [
		"Snippets",
		"Other",
		"Chat"
	],
	"icon": "resources/osmynt-128x.png",
	"contributes": {
		"commands": [
			{
				"command": "osmynt.login",
				"title": "Login",
				"category": "Osmynt",
				"icon": "$(github)"
			},
			{
				"command": "osmynt.logout",
				"title": "Logout",
				"category": "Osmynt",
				"icon": "$(github)"
			},
			{
				"command": "osmynt.refreshEntry",
				"title": "Refresh",
				"category": "Osmynt",
				"icon": "$(refresh)"
			},
			{
				"command": "osmynt.shareCode",
				"title": "Share Selected Code",
				"category": "Osmynt",
				"icon": "$(share)"
			},
			{
				"command": "osmynt.inviteMember",
				"title": "Invite Member",
				"category": "Osmynt",
				"icon": "$(plus)"
			},
			{
				"command": "osmynt.removeTeamMember",
				"title": "Remove Member",
				"category": "Osmynt",
				"icon": "$(trash)"
			},
			{
				"command": "osmynt.acceptInvitation",
				"title": "Accept Invitation",
				"category": "Osmynt",
				"icon": "$(check)"
			},
			{
				"command": "osmynt.viewSnippet",
				"title": "View Snippet",
				"category": "Osmynt",
				"icon": "$(code)"
			},
			{
				"command": "osmynt.refreshTeam",
				"title": "Refresh Team",
				"icon": "$(refresh)",
				"category": "Osmynt"
			},
			{
				"command": "osmynt.snippet.copy",
				"title": "Copy Snippet",
				"category": "Osmynt",
				"icon": "$(copy)"
			},
			{
				"command": "osmynt.snippet.openToSide",
				"title": "Open Snippet to Side",
				"category": "Osmynt",
				"icon": "$(file-code)"
			},
			{
				"command": "osmynt.snippet.insertAtCursor",
				"title": "Insert Snippet at Cursor",
				"category": "Osmynt",
				"icon": "$(insert)"
			},
			{
				"command": "osmynt.filterRecentsByMember",
				"title": "Filter Recents by This Member",
				"category": "Osmynt",
				"icon": "$(filter)"
			},
			{
				"command": "osmynt.clearRecentsFilter",
				"title": "Clear Recents Filter",
				"category": "Osmynt",
				"icon": "$(clear-all)"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "osmynt",
					"title": "Osmynt",
					"icon": "resources/osmynt.svg"
				}
			]
		},
		"views": {
			"osmynt": [
				{
					"id": "osmyntView",
					"name": "Osmynt",
					"contextualTitle": "Osmynt",
					"visibility": "visible",
					"when": "true"
				}
			]
		},
		"menus": {
			"editor/context": [
				{
					"command": "osmynt.shareCode",
					"group": "navigation",
					"when": "editor.hasSelection"
				}
			],
			"view/title": [
				{
					"command": "osmynt.refreshTeam",
					"when": "view == osmyntView",
					"group": "navigation@-1"
				}
			],
			"view/item/context": [
				{
					"command": "osmynt.snippet.copy",
					"when": "view == osmyntView && viewItem == snippetItem",
					"group": "inline@1"
				},
				{
					"command": "osmynt.snippet.openToSide",
					"when": "view == osmyntView && viewItem == snippetItem",
					"group": "inline@2"
				},
				{
					"command": "osmynt.snippet.insertAtCursor",
					"when": "view == osmyntView && viewItem == snippetItem",
					"group": "inline@3"
				},
				{
					"command": "osmynt.removeTeamMember",
					"when": "view == osmyntView && viewItem == memberItemOwner",
					"group": "inline@9"
				},
				{
					"command": "osmynt.filterRecentsByMember",
					"when": "view == osmyntView && viewItem == memberItem",
					"group": "navigation@1"
				},
				{
					"command": "osmynt.clearRecentsFilter",
					"when": "view == osmyntView && viewItem == teamItem",
					"group": "navigation@2"
				}
			]
		},
		"configuration": {
			"title": "Osmynt",
			"properties": {
				"osmynt.engineBaseUrl": {
					"type": "string",
					"default": "http://localhost:3000/osmynt-api-engine",
					"description": "Osmynt Engine base URL (e.g. https://api.osmynt.com/osmynt-api-engine)"
				},
				"osmynt.supabaseUrl": {
					"type": "string",
					"default": "https://tylxgkeecqxgaasmtitr.supabase.co",
					"description": "Supabase project URL for realtime subscriptions (optional)"
				},
				"osmynt.supabaseAnonKey": {
					"type": "string",
					"default": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6InR5bHhna2VlY3F4Z2Fhc210aXRyIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NTkyMjMzNDgsImV4cCI6MjA3NDc5OTM0OH0.nhFCGI3dWAq18nrBWl-fFvP-WZsaiEVWHnxxgGom8xk",
					"description": "Supabase anon key for realtime (optional)"
				},
				"osmynt.teamKeyMode": {
					"type": "boolean",
					"default": true,
					"description": "Use a single symmetric team key instead of per-recipient key wrapping."
				},
				"osmynt.teamKeyNamespace": {
					"type": "string",
					"default": "default",
					"description": "Namespace/key for team key storage (used when teamKeyMode is enabled)."
				}
			}
		}
	},
	"engines": {
		"vscode": "^1.94.0"
	}
}
